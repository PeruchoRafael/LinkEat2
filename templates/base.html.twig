<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}LinkEat{% endblock %}</title>
        
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100..900;1,100..900&display=swap" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        {% include 'component/included/_navbar.html.twig' %}

        {% block body %}
        {% endblock %}
    

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
            <script src="{{ asset('asset/script/script.js') }}"></script>
        {% endblock %}
        
        {% include 'component/included/_footer.html.twig' %}
    </body>
</html>
